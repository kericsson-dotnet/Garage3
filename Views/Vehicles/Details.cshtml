@model Garage.Models.Vehicle

<h1>Vehicle Details</h1>

<div>
    <h4>User Id: @Model.UserId</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Registration Number</dt>
        <dd class="col-sm-10">@Model.RegNumber</dd>
        
        <dt class="col-sm-2">Brand</dt>
        <dd class="col-sm-10">@Model.Brand</dd>
        
        <dt class="col-sm-2">Model</dt>
        <dd class="col-sm-10">@Model.Model</dd>
        
        <dt class="col-sm-2">Color</dt>
        <dd class="col-sm-10">@Model.Color</dd>
        
        <dt class="col-sm-2">Number of Wheels</dt>
        <dd class="col-sm-10">@Model.NumberOfWheels</dd>
        
        <dt class="col-sm-2">Vehicle Type</dt>
        <dd class="col-sm-10">@Model.VehicleType</dd>
    </dl>
</div>

