@model IEnumerable<Garage.Models.User>

<h1>Users</h1>

<table>
    <thead>
        <tr>
            <th>Personal Number</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age</th>
            <th>Vehicles</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.PersonalNumber</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Age</td>
            <td>@string.Join(", ", user.Vehicles.Select(v => v.RegNumber))</td>
        </tr>
    }
    </tbody>
</table>
